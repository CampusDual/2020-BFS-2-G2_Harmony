<JdbcEntitySetup
 xmlns="http://www.ontimize.com/schema/jdbc"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://www.ontimize.com/schema/jdbc http://www.ontimize.com/schema/jdbc/ontimize-jdbc-dao.xsd"
 catalog="" schema="${mainschema}" table="artist"
 datasource="mainDataSource" sqlhandler="dbSQLStatementHandler">
 <DeleteKeys>
  <Column>artist_id</Column>
 </DeleteKeys>
 <UpdateKeys>
  <Column>artist_id</Column>
 </UpdateKeys>
 <GeneratedKey>artist_id</GeneratedKey>
 <Queries>
	 <Query id="newestArtists">
			<AmbiguousColumns>
				<AmbiguousColumn name="artist_id" prefix="a"/>
			</AmbiguousColumns>
			<Sentence>
				<![CDATA[
				SELECT
					#COLUMNS#
				FROM
					${mainschema}.ARTIST a
				#WHERE#
				]]>
			</Sentence>
		</Query>
		<Query id="searchArtistById">
			<AmbiguousColumns>
				<AmbiguousColumn name="artist_id" prefix="a"/>
			</AmbiguousColumns>
			<Sentence>
				<![CDATA[
				SELECT
					#COLUMNS#
				FROM
					${mainschema}.ARTIST a
				#WHERE#
				]]>
			</Sentence>
		</Query>
		
 	<Query id = "searchArtist">
 		<Sentence>
 			<![CDATA[
 			SELECT
 			#COLUMNS#
 			FROM
 			${mainschema}.artist a  
 			#WHERE#
 			]]>
 	</Sentence>
 	</Query>	
 </Queries>
</JdbcEntitySetup>