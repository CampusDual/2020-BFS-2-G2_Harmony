 		</AmbiguousColumns>
 	<Sentence>
 	<![CDATA[
 	SELECT
 		#COLUMNS#
 	FROM
 		${mainschema}.song s
 	#WHERE#
 		
 	]]>
 	</Sentence>
 	</Query>
 </Queries>
</JdbcEntitySetup>